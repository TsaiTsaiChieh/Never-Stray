{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node build/index.js",
    "build": "tsc -p .",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "ts-node": "npx ts-node",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:generate": "typeorm migration:generate -n MigrationName -p",
    "typeorm:run": "typeorm migration:run",
    "init:area": "node build/scripts/init-area.js",
    "init:area:dev": "NODE_ENV=development ts-node src/scripts/init-area.ts",
    "init:shelter": "node build/scripts/init-shelter.js",
    "init:shelter:dev": "NODE_ENV=development ts-node src/scripts/init-shelter.ts" 
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/node": "^17.0.16",
    "class-transformer": "0.3.2",
    "class-validator": "0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "http-status": "^1.5.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8"
  }
}
